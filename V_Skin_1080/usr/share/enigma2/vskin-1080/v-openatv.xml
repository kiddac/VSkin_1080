; minwidth:	listwidth = minwidthif self["list"] and self.list is not None:	listcount = len(self.list)		if listcount &gt; 10:		listcount = 10if not self["text"].text:	textsize = (listwidth, 0)else:	textsizeoriginal = self["text"].getSize()	textsize = (listwidth, textsizeoriginal[1])listsize = (listwidth, itemheight*listcount)self["text"].instance.resize(eSize(*textsize))self["list"].instance.resize(eSize(*listsize))self["text"].instance.move(ePoint(0, 0))self["list"].instance.move(ePoint(0, textsize[1] + 30))wsizex = listwidthwsizey = textsize[1] + listsize[1] + 60wsize = (wsizex, wsizey)self.instance.resize(eSize(*wsize))self.instance.move(ePoint((desktop_w-wsizex)//2, (desktop_h-wsizey)//2))</applet>	</screen>	<!-- Console -->	<!-- /lib/python/Screens/Console.py -->	<screen name="Console" position="0,0" size="1920,1080" title="Console" flags="wfNoBorder" backgroundColor="transparent">		<panel name="v-template2-pig"/>		<widget name="text" position="129,216" size="1193,767" font="Console;24" foregroundColor="fg1" backgroundColor="bg-mm-dark" transparent="1" zPosition="3"/>	</screen>	<!-- CronManager (info panel ->  extensions -> cron manager, is different to timer menu -> cron timers) -->	<!-- /lib/python/Plugins/Extensions/Infopanel/CronManager.py -->	<screen name="CronTimers" position="0,0" size="1920,1080" flags="wfNoBorder" backgroundColor="transparent">		<panel name="v-template2-pig"/>		<widget name="lab1" position="129,215" size="300,60" font="Regular;36" foregroundColor="fg1" backgroundColor="bg-tl" valign="center" transparent="1" zPosition="2"/>		<widget name="labdisabled" position="399,215" size="180,60" font="Regular;36" foregroundColor="fg1" backgroundColor="red" valign="center" halign="center" transparent="0" zPosition="2"/>		<widget name="labactive" position="399,215" size="180,60" font="Regular;36" foregroundColor="fg1" backgroundColor="green" valign="center" halign="center" transparent="0" zPosition="2"/>		<widg